{
  "version": 2,
  "builds": [
    { "src": "web/next.config.js", "use": "@now/next" },
    { "src": "api/**/index.js", "use": "now-micro" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/event/(?<id>[^/]*)", "dest": "/web/?id=$id" },
    { "src": "/(.*)", "dest": "/web/$1" }
  ],
  "alias": "northportomenacalendar.com"
}
